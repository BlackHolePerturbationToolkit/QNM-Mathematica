Cell["\<\
The quasinormal mode frequencies are those of the Teukolsky radial equation:\
\>", "Notes",
 CellChangeTimes->{{3.897530370928481*^9, 3.8975303989698753`*^9}, {
   3.897530642631846*^9, 3.897530648449898*^9}, {3.897530815040489*^9, 
   3.897530859038587*^9}, 3.89753091195081*^9, 3.897530963151802*^9, {
   3.8975310964433208`*^9, 3.897531111999919*^9}, {3.897531229589902*^9, 
   3.897531236789856*^9}, {3.897531282723724*^9, 3.897531284018341*^9}, {
   3.897531408217187*^9, 3.897531409233102*^9}, {3.897531500339125*^9, 
   3.897531508044305*^9}, {3.9667798590339603`*^9, 3.966779900986951*^9}, {
   3.966780651862362*^9, 3.966780660427437*^9}, {3.966782225043001*^9, 
   3.966782276579363*^9}},
 CellID->801258952,ExpressionUUID->"8cf3ec66-27ad-4b67-9078-77cad43024a5"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         RowBox[{"[", 
           RowBox[{
             SuperscriptBox["\[CapitalDelta]", 
              RowBox[{"-", 
                StyleBox["s", "TI"]}]], 
             FractionBox[
              StyleBox["d", "TI"], 
              RowBox[{
                StyleBox["d", "TI"], 
                StyleBox["r", "TI"]}]], 
             RowBox[{"(", 
               RowBox[{
                 SuperscriptBox["\[CapitalDelta]", 
                  RowBox[{
                    StyleBox["s", "TI"], "+", "1"}]], 
                 FractionBox[
                  StyleBox["d", "TI"], 
                  RowBox[{
                    StyleBox["d", "TI"], 
                    StyleBox["r", "TI"]}]]}], ")"}], "+", 
             FractionBox[
              RowBox[{
                SuperscriptBox[
                 StyleBox["K", "TI"], "2"], "-", "2", 
                StyleBox["i", "TI"], 
                StyleBox["s", "TI"], 
                RowBox[{"(", 
                  RowBox[{
                    StyleBox["r", "TI"], "-", 
                    StyleBox["M", "TI"]}], ")"}], 
                StyleBox["K", "TI"]}], "\[CapitalDelta]"], "+", "4", 
             StyleBox["i", "TI"], 
             StyleBox["s", "TI"], "\[Omega]", 
             StyleBox["r", "TI"], "-", 
             SubscriptBox["\[Null]", 
              StyleBox["s", "TI"]], 
             SubscriptBox["\[Lambda]", 
              RowBox[{"\[ScriptL]", 
                StyleBox["m", "TI"]}]]}], "]"}], 
         SubscriptBox["\[Null]", 
          StyleBox["s", "TI"]], 
         SubscriptBox["\[Psi]", 
          RowBox[{"\[ScriptL]", 
            StyleBox["m", "TI"], "\[Omega]"}]], "\[LongEqual]", "0"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\bigg[\\Delta^{-s} \\frac{d}{dr} \\bigg( \\Delta^{s+1}\\frac{d \
}{dr}\\bigg)\n  +\\frac{K^2 - 2 i s (r-M)K}{\\Delta} + 4 i s \\omega r - {}_s \
\\lambda_{\\ell m} \\bigg]{}_{s} \\psi_{\\ell m \\omega} = 0", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],
  CellChangeTimes->{{3.897530966000611*^9, 3.897530973927291*^9}, 
    3.89753121956231*^9},ExpressionUUID->
  "31549947-7fb7-4d2d-b402-709c878462c6"],
 "\[LineSeparator]where ",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        StyleBox["K", "TI"], "\[LongEqual]", 
        RowBox[{"(", 
          RowBox[{
            SuperscriptBox[
             StyleBox["r", "TI"], "2"], "+", 
            SuperscriptBox[
             StyleBox["a", "TI"], "2"]}], ")"}], "\[Omega]", "-", 
        StyleBox["a", "TI"], 
        StyleBox["m", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
    "K=(r^2+a^2)\\omega - a m", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"]],
  CellChangeTimes->{{3.8975314235017443`*^9, 3.897531426346553*^9}},
  ExpressionUUID->"8533c4e7-52eb-4485-afc2-982b35694693"],
 ", ",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{"\[CapitalDelta]", "\[LongEqual]", 
        SuperscriptBox[
         StyleBox["r", "TI"], "2"], "-", "2", 
        StyleBox["M", "TI"], 
        StyleBox["r", "TI"], "+", 
        SuperscriptBox[
         StyleBox["a", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
    "input" -> "\\Delta = r^2 - 2 M r + a^2", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"]],
  CellChangeTimes->{3.897531450996215*^9},ExpressionUUID->
  "70c90f54-7292-4f8f-8ede-59317a5622c5"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|
    "boxes" -> FormBox["\[Omega]", TraditionalForm], "errors" -> {}, "input" -> 
     "\\omega", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "d038b69e-b010-4cae-9a74-5c95c515ee33"],
 " is a quasinormal mode frequency."
}], "Notes",
 CellChangeTimes->{{3.897530370928481*^9, 3.8975303989698753`*^9}, {
   3.897530642631846*^9, 3.897530648449898*^9}, {3.897530815040489*^9, 
   3.897530859038587*^9}, 3.89753091195081*^9, 3.897530963151802*^9, {
   3.897531096443321*^9, 3.897531111999919*^9}, {3.897531229589902*^9, 
   3.897531236789856*^9}, {3.897531282723724*^9, 3.897531284018341*^9}, {
   3.897531408217187*^9, 3.897531409233102*^9}, {3.897531500339125*^9, 
   3.897531508044305*^9}, {3.9667798590339603`*^9, 3.966779900986951*^9}, {
   3.966780651862362*^9, 3.966780682922672*^9}},
 CellID->511123774,ExpressionUUID->"d9dfbd1c-fa34-4c17-8b3f-e5b736f2d66a"],

Cell[TextData[{
 "For ",
 Cell[BoxData[
  RowBox[{"s", "=", "0"}]], "InlineFormula",ExpressionUUID->
  "830fbb41-62a1-498f-8ef7-1cdbfb742f69"],
 ",  the Teukolsky radial equation is equivalent to the scalar wave equation \
in Kerr spacetime.."
}], "Notes",
 CellChangeTimes->{
  3.89361716816155*^9, {3.893783707078019*^9, 3.8937837576083517`*^9}, 
   3.893783861823656*^9, {3.893783911596445*^9, 3.893783935592285*^9}, {
   3.897531384029173*^9, 3.89753140418285*^9}},
 CellID->835276902,ExpressionUUID->"e1928f41-d182-4608-8f23-898f0c9a0ac9"],

Cell[TextData[{
 Cell[BoxData[
  ButtonBox["QNMFrequency",
   BaseStyle->"Link",
   ButtonData->"paclet:QNM/ref/QNMFrequency"]], "InlineFormula",
  ExpressionUUID->"b2584422-5f28-4d12-ad05-6ab2176ff00f"],
 " can be evaluated to arbitrary numerical precision."
}], "Notes",
 CellChangeTimes->{{3.966780233589695*^9, 3.966780249503647*^9}, {
  3.966782297156386*^9, 3.966782306118123*^9}},
 CellID->303477332,ExpressionUUID->"7eb034f1-3698-417a-8eb1-715657811f7b"],

Cell[TextData[{
 "For numerical values of ",
 Cell[BoxData["a"], "InlineFormula",
  FormatType->StandardForm,ExpressionUUID->
  "6b72a8d9-f53c-474b-88f8-78f4c7d57170"],
 ", QNMFrequency aims to produce a numerical value with the maximum possible \
precision given the precision of ",
 Cell[BoxData["a"], "InlineFormula",
  FontFamily->"Source Sans Pro",ExpressionUUID->
  "7bb04ed6-ba49-4c43-a8ee-065e154fe569"],
 "."
}], "Notes",
 CellChangeTimes->{{3.9667827379426413`*^9, 3.9667827701814137`*^9}},
 CellID->1793794626,ExpressionUUID->"b4569d23-dcd6-4488-bfea-1a22fcf30993"],

Cell["The following options can be given: ", "Notes",
 CellID->2110378288,ExpressionUUID->"a771389c-6c67-4644-90fd-8891f8bcfe0f"],

Cell[BoxData[GridBox[{
   {Cell["   ", "ModInfo",ExpressionUUID->
     "2a0aaa4f-fc7e-444a-9b10-083dac1bcce6"], 
    ButtonBox["Method",
     BaseStyle->"Link"], 
    ButtonBox["Automatic",
     BaseStyle->"Link"], Cell["\<\
Method to use. Possible choices include: \"Hypeboloidal\",  \
\"Interpolation\", and \"IncidenceAmplitude\".\
\>", "TableText",ExpressionUUID->"a0f89267-2261-4fee-96e2-f35f51bc8742"]}
  }]], "3ColumnTableMod",
 CellChangeTimes->{{3.966779129521284*^9, 3.966779129521298*^9}},
 CellID->1287972564,ExpressionUUID->"e52beed7-5cdf-409e-b0e8-1929543e6d91"],

Cell[TextData[{
 "Frequencies computed using the \"Hyperboloidal\" method use the algorithm \
described in ",
 ButtonBox["\"Computing the quasinormal modes and eigenfunctions for the \
Teukolsky equation using horizon penetrating, hyperboloidally compactified \
coordinates\", Justin L. Ripley, Class. Quantum Grav. 39 145009 (2022)",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://doi.org/10.1088/1361-6382/ac776d"], None}],
 "."
}], "Notes",
 CellChangeTimes->{{3.897526502172883*^9, 3.897526519121022*^9}, {
  3.966780357119798*^9, 3.9667803832668324`*^9}, {3.966780997238843*^9, 
  3.9667810091734257`*^9}, {3.966781075915391*^9, 3.9667811082688727`*^9}, {
  3.9667823488227367`*^9, 3.9667823907793818`*^9}},
 CellID->1046863662,ExpressionUUID->"8d4bd30b-638c-41f9-913d-7cdcc5e94919"],

Cell[TextData[{
 "Frequencies computed using the \"IncidenceAmplitude\" method directly \
search for a zero of the Wronskian by evaluating the incidence amplitude of \
the \"in\" solution obtained using the ",
 ButtonBox["Teukolsky",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://bhptoolkit.org/Teukolsky"], None}],
 " package."
}], "Notes",
 CellChangeTimes->{{3.966782403124978*^9, 3.966782473993041*^9}},
 CellID->1839589043,ExpressionUUID->"80501770-c3e7-42db-881e-7ff4eb18e414"],

Cell[TextData[{
 "Frequencies compute using the \"Interpolation\" method are interpolated \
from tables of values included in ",
 ButtonBox["Leo Stein's qnm package",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://github.com/duetosymmetry/qnm"], None}],
 "."
}], "Notes",
 CellChangeTimes->{{3.9667824853553047`*^9, 3.9667825926530046`*^9}},
 CellID->854354429,ExpressionUUID->"81174722-88f5-49ba-b5f9-4d84c0084e6a"],

Cell["\<\
If an initial guess is not specified explicitly, then the \"Hyperboloidal\" \
and \"IncidenceAmplitude\" methods use the output of the \"Interpolation\" \
method as an initial guess.\
\>", "Notes",
 CellChangeTimes->{{3.966782596826791*^9, 3.966782644059823*^9}},
 CellID->1795948249,ExpressionUUID->"85a1fb94-37a4-4b31-bc9e-e6fcb69623f0"],

Cell[TextData[{
 "For Methods that support suboptions, they may be passed as a list. For \
example ",
 Cell[BoxData[
  RowBox[{"Method", "\[Rule]", 
   RowBox[{"{", 
    RowBox[{"\"\<Hyperboloidal\>\"", ",", " ", 
     RowBox[{"\"\<NumPoints\>\"", "\[Rule]", "30"}]}], "}"}]}]], 
  "InlineFormula",ExpressionUUID->"ad789a62-b302-49de-acf4-ef406562cb68"],
 " sets the \"NumPoints\" suboption for the \"Hyperboloidal\" method to 30."
}], "Notes",
 CellChangeTimes->{{3.966780407910036*^9, 3.9667804865715714`*^9}, {
   3.96678118742167*^9, 3.9667811898486977`*^9}, 3.9667826687616167`*^9},
 CellID->729522314,ExpressionUUID->"2a97cccc-e5ae-46ae-a4d7-6b0a142df8c9"],

Cell[TextData[{
 "The possible suboptions that the \"Hyperboloidal\" method accepts are: ",
 Cell[BoxData[
  RowBox[{"\"\<NumPoints\>\"", " ", "->", " ", "n"}]], "InlineFormula",
  ExpressionUUID->"86deb20f-5e57-4b29-ba23-47a7b0735f8e"],
 ", ",
 Cell[BoxData[
  RowBox[{"\"\<InitialGuess\>\"", " ", "->", " ", "\[Omega]"}]], 
  "InlineFormula",ExpressionUUID->"1f7a383d-8576-490f-a5e9-6db66f16d3fe"],
 " and ",
 Cell[BoxData[
  RowBox[{"\"\<AccuracyCheck\>\"", " ", "->", " ", "check"}]], 
  "InlineFormula",ExpressionUUID->"34da73d8-3154-4c5c-8ca0-52a1a24b86db"],
 " . Here, ",
 Cell[BoxData[
 "n"], "InlineFormula",ExpressionUUID->"00a9d5c6-0f4f-442d-a826-9789f122f4ff"],
 " specifies that the spectral grid should have n nodes, ",
 Cell[BoxData[
 "\[Omega]"], "InlineFormula",ExpressionUUID->
  "1bc4d2c4-db2d-4e97-b038-a3103f29eae7"],
 "  is the initial guess to use in the algorithm and ",
 Cell[BoxData["check"], "InlineFormula",
  FormatType->StandardForm,ExpressionUUID->
  "57ff07d6-1352-4394-9e86-fc02f92fd14c"],
 "  can be either True (the default) or False (for faster performance)."
}], "Notes",
 CellChangeTimes->{{3.966780471652137*^9, 3.966780629888081*^9}, {
  3.9667831006465693`*^9, 3.966783226859398*^9}},
 CellID->1569230924,ExpressionUUID->"52fe42c0-77c7-4d73-8125-488a59455d62"],

Cell[TextData[{
 "The possible suboptions that the \"Interpolation\" method accepts are the \
same as those of Mathematica's ",
 ButtonBox["Interpolation",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/Interpolation"],
 " function."
}], "Notes",
 CellChangeTimes->{{3.966780471652137*^9, 3.966780629888081*^9}, {
  3.9667831006465693`*^9, 3.966783273664974*^9}},
 CellID->433165670,ExpressionUUID->"92456aa0-6495-4d1a-b00f-665ebf65f3e0"],

Cell[TextData[{
 "The possible suboptions that the \"IncidenceAmplitude\" method accepts are: \
",
 Cell[BoxData[
  RowBox[{"\"\<InitialGuess\>\"", " ", "->", " ", "\[Omega]"}]], 
  "InlineFormula",ExpressionUUID->"2e901cb4-8df5-4ef6-9e4e-411e756e6317"],
 " . Here, ",
 Cell[BoxData[
 "\[Omega]"], "InlineFormula",ExpressionUUID->
  "673a2178-f492-430d-98e7-7404014a6b22"],
 "  is the initial guess to use in the algorithm."
}], "Notes",
 CellChangeTimes->{{3.966780471652137*^9, 3.966780629888081*^9}, {
  3.9667831006465693`*^9, 3.966783226859398*^9}, {3.9667832862809*^9, 
  3.966783321167912*^9}},
 CellID->1123008647,ExpressionUUID->"a1bd859b-6afb-4cbb-ac05-67e46908fef6"],

Cell["\<\
The \"IncidenceAmplitude\" method is the slowest, but can produce the most \
accurate results.\
\>", "Notes",
 CellChangeTimes->{{3.966783329882502*^9, 3.966783349950075*^9}},
 CellID->1965912205,ExpressionUUID->"343f8b72-473f-4128-bc7b-7db9eac85be0"]
}, Open  ]],


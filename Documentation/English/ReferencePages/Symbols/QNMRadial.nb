Cell[TextData[{
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         RowBox[{"[", 
           RowBox[{
             SuperscriptBox["\[CapitalDelta]", 
              RowBox[{"-", 
                StyleBox["s", "TI"]}]], 
             FractionBox[
              StyleBox["d", "TI"], 
              RowBox[{
                StyleBox["d", "TI"], 
                StyleBox["r", "TI"]}]], 
             RowBox[{"(", 
               RowBox[{
                 SuperscriptBox["\[CapitalDelta]", 
                  RowBox[{
                    StyleBox["s", "TI"], "+", "1"}]], 
                 FractionBox[
                  StyleBox["d", "TI"], 
                  RowBox[{
                    StyleBox["d", "TI"], 
                    StyleBox["r", "TI"]}]]}], ")"}], "+", 
             FractionBox[
              RowBox[{
                SuperscriptBox[
                 StyleBox["K", "TI"], "2"], "-", "2", 
                StyleBox["i", "TI"], 
                StyleBox["s", "TI"], 
                RowBox[{"(", 
                  RowBox[{
                    StyleBox["r", "TI"], "-", 
                    StyleBox["M", "TI"]}], ")"}], 
                StyleBox["K", "TI"]}], "\[CapitalDelta]"], "+", "4", 
             StyleBox["i", "TI"], 
             StyleBox["s", "TI"], "\[Omega]", 
             StyleBox["r", "TI"], "-", 
             SubscriptBox["\[Null]", 
              StyleBox["s", "TI"]], 
             SubscriptBox["\[Lambda]", 
              RowBox[{"\[ScriptL]", 
                StyleBox["m", "TI"]}]]}], "]"}], 
         SubscriptBox["\[Null]", 
          StyleBox["s", "TI"]], 
         SubscriptBox["\[Psi]", 
          RowBox[{"\[ScriptL]", 
            StyleBox["m", "TI"], "\[Omega]"}]], "\[LongEqual]", "0"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "\\bigg[\\Delta^{-s} \\frac{d}{dr} \\bigg( \\Delta^{s+1}\\frac{d \
}{dr}\\bigg)\n  +\\frac{K^2 - 2 i s (r-M)K}{\\Delta} + 4 i s \\omega r - {}_s \
\\lambda_{\\ell m} \\bigg]{}_{s} \\psi_{\\ell m \\omega} = 0", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],
  CellChangeTimes->{{3.897530966000611*^9, 3.897530973927291*^9}, 
    3.89753121956231*^9},ExpressionUUID->
  "e743af20-40cc-41e1-9b6f-f29ef9768312"],
 "\[LineSeparator]where ",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        StyleBox["K", "TI"], "\[LongEqual]", 
        RowBox[{"(", 
          RowBox[{
            SuperscriptBox[
             StyleBox["r", "TI"], "2"], "+", 
            SuperscriptBox[
             StyleBox["a", "TI"], "2"]}], ")"}], "\[Omega]", "-", 
        StyleBox["a", "TI"], 
        StyleBox["m", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
    "K=(r^2+a^2)\\omega - a m", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"]],
  CellChangeTimes->{{3.8975314235017443`*^9, 3.897531426346553*^9}},
  ExpressionUUID->"e292b273-5b5b-428e-97ed-a8fab5121447"],
 ", ",
 Cell[BoxData[
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{"\[CapitalDelta]", "\[LongEqual]", 
        SuperscriptBox[
         StyleBox["r", "TI"], "2"], "-", "2", 
        StyleBox["M", "TI"], 
        StyleBox["r", "TI"], "+", 
        SuperscriptBox[
         StyleBox["a", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
    "input" -> "\\Delta = r^2 - 2 M r + a^2", "state" -> "Boxes"|>,
   "TeXAssistantTemplate"]],
  CellChangeTimes->{3.897531450996215*^9},ExpressionUUID->
  "ce8859a4-3f1e-4a37-9306-e9aedfd62310"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|
    "boxes" -> FormBox["\[Omega]", TraditionalForm], "errors" -> {}, "input" -> 
     "\\omega", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "153af37b-a204-4e29-8a52-efc4e301faf5"],
 " is a quasinormal mode frequency."
}], "Notes",
 CellChangeTimes->{{3.897530370928481*^9, 3.8975303989698753`*^9}, {
   3.897530642631846*^9, 3.897530648449898*^9}, {3.897530815040489*^9, 
